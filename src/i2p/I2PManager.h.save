#include <QObject>
#pragma once

#include <QObject>
#include <QString>

class I2PManager : public QObject
{
    Q_OBJECT

public:
    explicit I2PManager(QObject *parent = nullptr);

    // Checks if the daemon is reachable via the current I2P proxy.
    Q_INVOKABLE bool i2pStatus() const;

    // Reconfigure the wallet to use the I2P proxy and remote daemon address.
    Q_INVOKABLE void setProxyForI2p(const QString &i2pAddress);

    // Run a script to install/configure i2pd and write anoninbound/txproxy settings.
    Q_INVOKABLE void createI2pNode(const QString &password);
};#pragma once

